# Required
DATABASE_URL="postgresql://user:password@host:port/database?sslmode=require"

# Vercel OAuth (use either VERCEL_CLIENT_* or CLIENT_*)
VERCEL_CLIENT_ID="your_vercel_oauth_client_id"
VERCEL_CLIENT_SECRET="your_vercel_oauth_client_secret"
# Or legacy names:
# CLIENT_ID="your_vercel_oauth_client_id"  
# CLIENT_SECRET="your_vercel_oauth_client_secret"

# Optional
NEXT_PUBLIC_VERCEL_URL="your-app.vercel.app"