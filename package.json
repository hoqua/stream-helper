{
  "name": "@durablr/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run @durablr/web:dev & nx run @durablr/api:serve --watch",
    "dev:web": "nx dev web",
    "dev:api": "env-cmd -f apps/web/.env.local nx serve @durablr/api --watch",
    "-----TOOLS-------": "-----TOOLS-------",
    "stream": "tsx scripts/register-stream.ts",
    "-----QUALITY-------": "-----QUALITY-------",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all -- --fix",
    "-------DATABASE-------": "-------DATABASE-------",
    "db:generate": "env-cmd -f apps/web/.env.local drizzle-kit generate",
    "db:migrate": "env-cmd -f apps/web/.env.local drizzle-kit migrate",
    "db:push": "env-cmd -f apps/web/.env.local drizzle-kit push",
    "db:studio": "env-cmd -f apps/web/.env.local drizzle-kit studio",
    "-----DEPLOY-------": "-----DEPLOY-------",
    "db:migrate:prod": "drizzle-kit generate && drizzle-kit migrate",
    "build:web:prod": "nx build web",
    "build:api:prod": "nx build @durablr/api --configuration=production"
  },
  "engines": {
    "node": ">=22.0.0"
